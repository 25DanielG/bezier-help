{
  "name": "bezier-help",
  "publisher": "25DanielG",
  "displayName": "Bezier Help",
  "description": "An extension that helps developers create varied animation with an interactive gui displaying bezier graphs in CSS.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "enableWebview": true,
  "categories": [
    "Other"
  ],
  "extensionKind": [
		"ui",
		"workspace"
	],
  "keywords": [
		"bezier",
		"curve",
		"webview",
		"interactive",
    "css"
	],
  "repository": {
    "type": "git",
    "url": "https://github.com/25DanielG/bezier-help"
  },
  "activationEvents": [
    "onCommand:bezier-help.editBezier"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "bezier-help.editBezier",
        "title": "Edit Bezier Curve",
        "category": "Bezier Help"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.2"
  }
}
